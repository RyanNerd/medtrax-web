{
   "extends": ["tslint:recommended", "tslint-config-prettier"],
   "rules": {
      "max-line-length": {
         "options": [120]
      },
      "indent": [true, "spaces", 4],
      "no-console": false,
      "no-empty": false,
      "no-shadowed-variable": false,
      "no-namespace": false,
      "radix": false
   }
}
